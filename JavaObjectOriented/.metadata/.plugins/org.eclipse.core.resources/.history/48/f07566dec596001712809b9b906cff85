
public class FarmMain {
	
	public static void main(String args[]){
		//set up a farm
		
		Farm homeFarm = new Farm();
		
		homeFarm.farmerName = "Mohammed";
		homeFarm.numberOfCows = 10;
		homeFarm.numberOfGoats = 15;
		homeFarm.balesOfHay =50;
		
